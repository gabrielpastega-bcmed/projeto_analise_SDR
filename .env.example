# BigQuery Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# GCP Project ID
# BIGQUERY_PROJECT_ID=your-project-id

# BigQuery
BIGQUERY_PROJECT_ID=your_project_id
BIGQUERY_DATASET=your_dataset
BIGQUERY_TABLE=your_table
GOOGLE_APPLICATION_CREDENTIALS=path/to/credentials.json

# ============================================
# AUTHENTICATION DATABASE (PostgreSQL)
# ============================================
# PostgreSQL database for user authentication
# Use Railway, Supabase, or your own PostgreSQL instance
AUTH_DATABASE_HOST=your-postgres-host.railway.app
AUTH_DATABASE_PORT=5432
AUTH_DATABASE_NAME=railway
AUTH_DATABASE_USER=postgres
AUTH_DATABASE_PASSWORD=your-secure-password
AUTH_DATABASE_SCHEMA=public
GOOGLE_APPLICATION_CREDENTIALS=credentials/bigquery-key.json

# Analysis Settings
ANALYSIS_DAYS=7

# Google Gemini API (for qualitative analysis)
GEMINI_API_KEY=your_gemini_api_key

# ============================================
# AUTHENTICATION DATABASE (PostgreSQL)
# ============================================
AUTH_DATABASE_HOST=seu_host_aqui
AUTH_DATABASE_PORT=5432
AUTH_DATABASE_NAME=seu_database_aqui
AUTH_DATABASE_USER=seu_usuario_aqui
AUTH_DATABASE_PASSWORD=sua_senha_aqui
